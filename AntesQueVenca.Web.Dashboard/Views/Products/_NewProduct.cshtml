
<div class="modal fade bd-example-modal-lg  md-effect-13" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myLargeModalLabel">CADASTRAR PRODUTO</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="col-md-12">
                    <form>
                        <div class="row">
                            <div class="form-group col-12">
                                <label for="exampleInputEmail1">Produto</label>
                                <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Produto">
                            </div>

                            <div class="form-group col-6">
                                <label for="exampleInputPassword1">Quantidade</label>
                                <input type="number" class="form-control" id="exampleInputPassword1" placeholder="Quantidade">
                            </div>
                            <div class="col-md-6 mb-3">
                                <label for="validationTooltipUsername">Valor (Unitário)</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">$</span>
                                    </div>
                                    <input type="text" class="form-control autonumber" data-a-sep="." data-a-dec="," placeholder="Valor" aria-describedby="validationTooltipUsernamePrepend" >
                                </div>
                            </div>
                            <div class="form-group col-6">
                                    <p>Categorias</p>
                                    <select class="form-control col-sm-12" >
                                        <option value="AL">Achocolatado</option>
                                        <option value="WY">Leite em Pó</option>
                                        <option value="WY">Sabão em Pó</option>
                                        <option value="WY">Biscoito</option>
                                    </select>
                            </div>
                            <div class="form-group col-12">
                                <button type="submit" class="btn btn-primary">Submit</button>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
